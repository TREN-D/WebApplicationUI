{
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules": {
    "linebreak-style": 0,
    "comma-dangle": ["error", "never"],
    "react/jsx-one-expression-per-line": 0,
    "react/prefer-stateless-function": 0
  },
  "env": {
    "amd": true,
    "es6": true,
    "browser": true,
    "node": true
  },
  "extends": "airbnb"
}